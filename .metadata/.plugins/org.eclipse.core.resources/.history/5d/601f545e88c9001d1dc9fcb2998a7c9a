package bean.pojo;

public  class Ogrenci { // soyut bir class haline getidik artık doğrudan bir obje oluştuulmasın .
	
private String no;
private String ad;
private String soyad ;
private int sinif ;
private char sube ;

public Ogrenci() {
	
	
}


public Ogrenci(String no, String ad, String soyad, int sinif, char sube) {

	this.no = no;
	this.ad = ad;
	this.soyad = soyad;
	this.sinif = sinif;
	this.sube = sube;
} 


public String getNo() {
	return no;
}
public void setNo(String no) {
	this.no = no;
}
public String getAd() {
	return ad;
}
public void setAd(String ad) {
	this.ad = ad;
}
public String getSoyad() {
	return soyad;
}
public void setSoyad(String soyad) {
	this.soyad = soyad;
}
public int getSinif() {
	return sinif;
}
public void setSinif(int sinif) {
	this.sinif = sinif;
}
public char getSube() {
	return sube;
}
public void setSube(char sube) {
	this.sube = sube;
}




}
